<h2><%= @eatery.name %></h2>
  <h3>Address</h3>
    <p>
      <%= @eatery.address %>
    </p>
  <h3>Cuisine</h3>
    <p>
      <%= @eatery.cuisine %>
    </p>
  <h3>Visitors</h3>
    <ul>
      <% @eatery.users.each do |user| %>
        <li><%= link_to "#{user.username}", user_path(user) %></li>
      <% end %>
    </ul>

<%= link_to 'Edit', edit_eatery_path(@eatery) %>
<%= link_to 'Back', eateries_path %>