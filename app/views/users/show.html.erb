<h2>Welcome <%= @user.username %>!</h2>
<h3>Email: </h3>
  <p>
    <%= @user.email %>
  </p>

<h3>Agendas</h3>
  <% @user.agendas.each do |agenda| %>
  <p>
    <b>Eatery:</b>
    <%= agenda.eatery.name %>
    <b>Attraction:</b>
    <%= agenda.attraction.name %>
  </p>
  <br>
</p>
  <% end %>

  <%= link_to 'Sign Out', destroy_user_session_path %>